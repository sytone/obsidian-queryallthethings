  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "conventionalCommits.scopes": [
    "logging",
    "render",
    "query",
    "docs"
  ],
  "markdownlint.config": {
    "MD007": {
      "indent": 2
    },
    "single-trailing-newline": false
  }
}
