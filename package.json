{
  "name": "qatt",
  "version": "0.10.0",
  "description": "Execute SQL base queries against your data in Obsidian and render it how you want using templates.",
  "main": "main.js",
  "scripts": {
    "build:dev": "node ophidian.config.mjs dev",
    "build": "node ophidian.config.mjs production && mdinject --root=./src --docs-root=./docs --wrap",
    "build:oldprod": "node .config/esbuild.config.mjs production && mdinject --root=./src --docs-root=./docs --wrap",
    "build:olddev": "node .config/esbuild.config.mjs",
    "build:docs": "mdinject --root=./src --docs-root=./docs --wrap",
    "lint": "xo --fix",
    "lint:markdown": "markdownlint-cli2-config \".config/.markdownlint-cli2.jsonc\" \"**/*.md\" \"#node_modules\" \"#CHANGELOG.md\" \"#docs/CHANGELOG.md\"",
    "test": "jest",
    "test:dev": "cross-env NODE_OPTIONS=--experimental-vm-modules pnpm exec jest --watch",
    "prerelease": "xo --fix && mdinject --root=./src --docs-root=./docs --wrap && mocha --ci",
    "release": "standard-version",
    "release:dry": "standard-version --dry-run",
    "release:major": "standard-version --release-as major --no-verify",
    "release:minor": "standard-version --release-as minor --no-verify",
    "release:minor:dry": "standard-version --release-as minor --no-verify --dry-run",
    "release:patch": "standard-version --release-as patch --no-verify"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "obsidian-query"
  ],
  "author": "Sytone",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.3",
    "@jest/globals": "^29.7.0",
    "@ophidian/build": "^1.2.1",
    "@ophidian/core": "^0.0.21",
    "@preact/signals-core": "^1.6.0",
    "@rollup/plugin-commonjs": "25.0.2",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.1",
    "@sytone/markdown-snippet-injector": "^2.2.0",
    "@types/codemirror": "^5.60.15",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "builtin-modules": "3.3.0",
    "commitizen": "^4.3.0",
    "compare-versions": "^6.1.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "0.18.5",
    "esbuild-plugin-copy-file": "^0.0.2",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nyc": "^15.1.0",
    "obsidian": "1.2.8",
    "prettier": "2.8.8",
    "rollup": "^3.29.4",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "2.5.3",
    "typescript": "5.1.3",
    "xo": "^0.54.2"
  },
  "dependencies": {
    "alasql": "^4.3.3",
    "eventemitter2": "^6.4.9",
    "handlebars": "^4.7.8",
    "luxon": "^3.4.4",
    "mdast-util-from-markdown": "^1.3.1",
    "mdast-util-to-string": "^3.2.0",
    "mdast-util-wiki-link": "^0.0.2",
    "micromark": "^3.2.0",
    "micromark-extension-gfm": "^2.0.3",
    "micromark-extension-gfm-footnote": "^1.1.2",
    "micromark-extension-gfm-task-list-item": "^1.0.5",
    "micromark-extension-math": "^2.1.2",
    "micromark-extension-wiki-link": "^0.0.4",
    "micromark-util-combine-extensions": "^1.1.0",
    "micromark-util-types": "^1.1.0",
    "obsidian-dataview": "0.5.56",
    "papaparse": "^5.4.1"
  },
  "xo": {
    "space": true,
    "rules": {
      "@typescript-eslint/naming-convention": [
        "error",
        {
          "selector": "interface",
          "format": [
            "PascalCase"
          ],
          "custom": {
            "regex": "^I[A-Z]",
            "match": true
          }
        }
      ],
      "@typescript-eslint/consistent-type-definitions": [
        "error",
        "interface"
      ],
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "pascalCase": true
          },
          "ignore": [
            "^.+\\.(test|spec).ts$"
          ]
        }
      ]
    }
  },
  "standard-version": {
    "tag-prefix": "",
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "change",
        "section": "Bug Fixes and Changes"
      },
      {
        "type": "fix",
        "section": "Bug Fixes and Changes"
      },
      {
        "type": "deprecate",
        "section": "Bug Fixes and Changes"
      },
      {
        "type": "build",
        "section": "Internal"
      },
      {
        "type": "chore",
        "section": "Internal"
      },
      {
        "type": "ci",
        "section": "Internal"
      },
      {
        "type": "docs",
        "section": "Documentation"
      },
      {
        "type": "style",
        "section": "Internal"
      },
      {
        "type": "refactor",
        "section": "Internal"
      },
      {
        "type": "revert",
        "section": "Internal"
      },
      {
        "type": "security",
        "section": "Internal"
      },
      {
        "type": "perf",
        "section": "Internal"
      },
      {
        "type": "test",
        "section": "Internal"
      }
    ],
    "commitUrlFormat": "https://github.com/sytone/obsidian-queryallthethings/commits/{{hash}}",
    "compareUrlFormat": "https://github.com/sytone/obsidian-queryallthethings/compare/{{previousTag}}...{{currentTag}}",
    "issueUrlFormat": "https://github.com/sytone/obsidian-queryallthethings/issues/{{id}}",
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      },
      {
        "filename": "manifest.json",
        "type": "json"
      },
      {
        "filename": "versions.json",
        "updater": ".config/obsidian-versions-updater.js"
      }
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}